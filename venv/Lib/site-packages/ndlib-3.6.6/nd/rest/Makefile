ci:
	svn ci -m"接口基础框架更新提交"

git:
	find -name '*.pyc' | xargs -i rm -f {}
	rm -rf test_reports
	git add $(GIT_DIR)
	git commit $(GIT_DIR) -m"$(GIT_DIR)更新"
	git push origin master

up:
	git pull
